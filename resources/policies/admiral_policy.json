{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-cloudinit/",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-cloudinit/*",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-config",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-config/*",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-dockerhub",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-dockerhub/*",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-jenkins",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-jenkins/*",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-splunk",
        "arn:aws:s3:::AWS-ACCOUNT-coreos-cluster-splunk/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "ec2:Describe*",
        "autoscaling:Describe*"
      ],
      "Resource": [
        "*"
      ]
    }
  ]
}
